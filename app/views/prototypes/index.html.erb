<main class="main">
  <div class="inner">
    <%# ログインしているときは以下を表示する %>
    <% if user_signed_in? %>
      <div class="greeting">
        <%= link_to "こんにちは、#{current_user.name}さん", user_path(current_user.id), class: :greeting__link %>  <%# リンクをテキスト全てに反映するテスト %>
      </div>
    <%# // ログインしているときは上記を表示する %>
    <% end %>
    <div class="card__wrapper">
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
      <%= render partial: "prototype", collection: @prototypes, as: "protable" %> <%# asで変数の値を指定できる、デフォルトはpartialの指定名%>
    </div>
  </div>
</main>

